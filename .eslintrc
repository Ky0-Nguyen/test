{
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "standard"
    ],
    "env": {
        "jest": true
    },
    "plugins": [
        "flowtype-errors",
        "react",
        "react-native"
    ],
    "parserOptions": {
        "ecmaFeatures": {
        "jsx": true,
        "filter" :true
        }
    },
    "rules": {
        "flowtype-errors/show-errors": 2,
        "react/prop-types": 0,
        "no-console": 0,
        "react/no-string-refs": 0,
        "no-undef": 0,
        "indent": [2,2]
    }
}